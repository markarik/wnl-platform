{
  "name": "wnl-platform",
  "description": "The main app for delivering medical e-learning courses. Based on Laravel.",
  "license": "GPL-3.0",
  "type": "project",
  "require": {
	"php": ">=7.0.0",
	"laravel/framework": "5.5.*",
	"kris/laravel-form-builder": "^1.10",
	"laracasts/utilities": "~2.0",
	"league/fractal": "^0.15.0",
	"guzzlehttp/guzzle": "^6.2",
	"barryvdh/laravel-dompdf": "^0.8.0",
	"sentry/sentry-laravel": "^0.6.1",
	"predis/predis": "^1.1",
	"league/flysystem-aws-s3-v3": "^1.0",
	"intervention/image": "^2.3",
	"laravel/scout": "^3.0",
	"bethink/elastic-scout": "3.*",
	"doctrine/dbal": "2.5.13",
    "graham-campbell/throttle": "^6.0",
	"nao-pon/flysystem-google-drive": "~1.1",
    "bschmitt/laravel-amqp": "^1.2"
  },
  "require-dev": {
	"laravel/dusk": "^2.0",
	"laravel/tinker": "^1.0",
	"fzaninotto/faker": "~1.4",
	"mockery/mockery": "0.9.*",
	"phpunit/phpunit": "~6.0",
	"symfony/css-selector": "3.1.*",
	"symfony/dom-crawler": "3.1.*",
	"barryvdh/laravel-debugbar": "^2.3",
	"filp/whoops": "~2.0",
    "barryvdh/laravel-ide-helper": "dev-master#99226c92b72b02bf60d9b339fdf9f446d5d9921d"
  },
  "autoload": {
	"classmap": [
	  "database"
	],
	"psr-4": {
	  "App\\": "app/",
	  "Demo\\App\\": "demo/app/",
	  "Lib\\": "lib/",
	  "Checks\\": "checks/"
	}
  },
  "autoload-dev": {
	"psr-4": {
	  "Tests\\": "tests/"
	}
  },
  "scripts": {
	"post-root-package-install": [
	  "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
	],
	"post-create-project-cmd": [
	  "php artisan key:generate"
	],
	"post-install-cmd": [
	  "Illuminate\\Foundation\\ComposerScripts::postInstall",
	  "php artisan optimize"
	],
	"post-update-cmd": [
	  "Illuminate\\Foundation\\ComposerScripts::postUpdate",
	  "php artisan optimize"
	]
  },
  "config": {
	"preferred-install": "dist"
  }
}
